{
    "$schema": "vscode://schemas/color-theme",
    "name": "Light (Visual Studio)",
    //"type": "vs",
    "semanticHighlighting": true,
    // https://github.com/microsoft/vscode/blob/master/extensions/theme-defaults/themes/dark_vs.json
	
    "semanticTokenColors": {
		"keyword.state":"#d1ca66",
		"keyword.statement": "#cf8a8a",
		"keyword.value": "#d0d0f0",
		"comment": "#49cf8c",
		"keyword.action": "#ecb9e4",
		"keyword.finalStates": "#bdffdb",
		"keyword.tape": "#7aa4ff"
	}
}